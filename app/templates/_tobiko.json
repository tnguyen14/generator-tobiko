{
    "port": "<%= localPort %>",
    "livereload": "<%= livereloadPort %>",
    "buildPath": "<%= buildDir %>/www",
    "contentDir": "<%= contentDir %>",
    "dataPath": "<%= buildDir %>/data.json",
    "sassDir": "<%= sassDir %>",
    "templates": {
        "dir": "<%= templatesDir %>",
        "partialDir": "<%= templatesDir %>/<%= templatesPartialsDir %>",
        "helperDir": "<%= templatesDir %>/<%= templatesHelpersDir %>"
    },
    "paginate": [
        {
            "dir": "posts",
            "postPerPage": 4,
            "template": "archive.hbs",
            "title": "Posts"
        }
    ]
}
